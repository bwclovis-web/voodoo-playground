{
  "name": "voodoo-stack",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "create:component": "node generator/create-component.cjs",
    "db:push": "prisma db push",
    "db:studio": "run-p prisma:studio",
    "db:generate": "prisma generate",
    "db:docs": "prisma generate && prisma-docs-generator serve start http://localhost:5858",
    "db:serve": "prisma db pull && prisma db push && run-p db:studio db:docs dev",
    "dev": "run-p start:server open:localhost",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "test": "vitest",
    "start": "cross-env NODE_ENV=production node ./server.mjs",
    "open:localhost": "start http://localhost:2112",
    "prisma:studio": "prisma studio",
    "start:server": "node ./server.mjs",
    "typecheck": "tsc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
